{
  "name": "crm-foundaction",
  "version": "1.1.1",
  "private": true,
  "description": "CRM-FOUNDATION-SYSTEM",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "build:dev": "cross-env REACT_APP_ENV=dev umi build",
    "build:master": "cross-env REACT_APP_ENV=master umi build",
    "build:uat": "cross-env REACT_APP_ENV=uat umi build",
    "build:test": "cross-env REACT_APP_ENV=test umi build",
    "dev": "npm run start:dev",
    "start": "cross-env REACT_APP_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev COMPRESS=none MOCK=none umi dev",
    "model": "umi dva list model",
    "precommit": "lint-staged",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "ahooks": "^2.9.4",
    "classnames": "^2.2.6",
    "immer": "^7.0.14",
    "js-cookie": "^2.2.1",
    "moment": "^2.25.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "umi": "^3.5.21",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout":"^6.33.0",
    "@ant-design/pro-table":"^2.64.1",
    "@ant-design/pro-form": "^1.55.0",
    "antd": "^4.19.2",
    "react-dev-inspector": "^1.7.1",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "nanoid": "^3.1.20",
    "umi-request": "^1.3.9",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@types/react-color": "^3.0.5",
    "@types/node": "^16.7.1",
    "@types/classnames": "^2.3.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/mockjs": "^1.0.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric":"^2.6.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-react": "^1.8.22",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-openapi": "^1.2.0",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-dumi": "^1.1.7",
    "@umijs/yorkie": "^2.0.3",
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "lint-staged": {
    "**/*.less": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  }
}
